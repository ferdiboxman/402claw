{
  "name": "402claw-dispatcher-prototype",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Workers-for-Platforms dispatcher prototype for 402claw",
  "scripts": {
    "test": "node --test",
    "test:d1:integration": "RUN_WRANGLER_D1_INTEGRATION=1 node --test ./tests/d1-wrangler-integration.test.js",
    "d1:create": "npx wrangler d1 create clawr-prod",
    "d1:migrate:local": "npx wrangler d1 execute CONTROL_DB --local --persist-to ./.wrangler-d1 --config ./wrangler.d1.local.toml --file ./migrations/0001_initial.sql",
    "d1:verify:local": "npx wrangler d1 execute CONTROL_DB --local --persist-to ./.wrangler-d1 --config ./wrangler.d1.local.toml --command \"SELECT name, type FROM sqlite_master ORDER BY name;\"",
    "d1:migrate:remote": "npx wrangler d1 execute clawr-prod --remote --file ./migrations/0001_initial.sql"
  }
}
